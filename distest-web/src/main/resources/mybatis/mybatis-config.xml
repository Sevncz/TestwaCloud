<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--<settings>-->
        <!--<setting name="logImpl" value="Slf4j" />-->
    <!--</settings>-->
    <typeAliases>
        <typeAlias alias="Integer" type="java.lang.Integer" />
        <typeAlias alias="Long" type="java.lang.Long" />
        <typeAlias alias="HashMap" type="java.util.HashMap" />
        <typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap" />
        <typeAlias alias="ArrayList" type="java.util.ArrayList" />
        <typeAlias alias="LinkedList" type="java.util.LinkedList" />
    </typeAliases>
    <plugins>
        <plugin interceptor="com.testwa.distest.common.plugin.PrepareInterceptor"/>
        <plugin interceptor="com.testwa.core.base.mybatis.plugin.ResultMapInterceptor"/>
        <plugin interceptor="com.testwa.core.base.mybatis.plugin.MyBatisShowSQLInterceptor"/>
        <plugin interceptor="com.testwa.core.base.mybatis.plugin.OptimisticLocker">
            <property name="versionColumn" value="lock_version"/>
        </plugin>
    </plugins>
</configuration>