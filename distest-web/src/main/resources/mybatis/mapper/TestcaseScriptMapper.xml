<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.testwa.distest.server.mvc.mapper.TestcaseScriptMapper" >
    <resultMap id="BaseResultMap" type="com.testwa.distest.server.entity.TestcaseScript" >
        <id column="id" property="id" jdbcType="BIGINT" />
        <result column="testcaseId" property="testcaseId" jdbcType="BIGINT" />
        <result column="scriptId" property="scriptId" jdbcType="BIGINT" />
        <result column="seq" property="seq" jdbcType="INTEGER" />
    </resultMap>

    <sql id="Base_Column_List" >
        id, testcaseId, scriptId
    </sql>

    <delete id="deleteByTestcaseId" parameterType="java.util.Map">
        DELETE FROM
        testcase_script
        WHERE testcaseId = #{testcaseId}
    </delete>

</mapper>